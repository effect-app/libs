{
  "name": "@effect-ts-app/fluent",
  "version": "0.40.0",
  "license": "MIT",
  "exports": {
    "./_ext/*": {
      "module": "./_esm/_ext/*.js",
      "default": "./_ext/*.js"
    },
    "./polyfill": {
      "module": "./_esm/polyfill/*.js",
      "default": "./polyfill/*.js"
    },
    "./polyfill/*": {
      "module": "./_esm/polyfill/*.js",
      "default": "./polyfill/*.js"
    },
    "./types/*": "./types/*.ts"
  },
  "devDependencies": {
    "@effect-ts/fluent": "^0.3.12",
    "@effect-ts/fluent-node": "^0.1.2",
    "@effect-ts/node": "^0.29.0"
  },
  "peerDependencies": {
    "@effect-ts/fluent": "^0.3.12",
    "@effect-ts/fluent-node": "^0.1.2",
    "@effect-ts/node": "^0.29.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --build && tsc --build tsconfig.cjs.json",
    "watch": "tsc --build --watch",
    "watch2": "tsc -w",
    "prepare": "yarn build",
    "clean": "rm -rf ./_esm && find . | egrep '\\.m?js(\\.map)?$|\\.d.ts(\\.map)?$|\\.tsbuildinfo' | grep -v ./types | grep -v .eslint | grep -v jest.config | xargs rm -f",
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./src",
    "compile": "tsc --noEmit",
    "lint": "eslint ./src ./types",
    "prettier": "prettier \"src/**/*.ts\" \"types/**/*.d.ts\"",
    "autofix": "yarn lint --fix && yarn prettier --write",
    "test": "echo 'no tests yet'",
    "testsuite": "yarn lint && yarn test && yarn circular",
    "ncu": "ncu"
  }
}
