      "types": "./dist/errorReporter.d.ts",
      "default": "./dist/errorReporter.js"
    },
    "./experimental/makeExperimental": {
      "types": "./dist/experimental/makeExperimental.d.ts",
      "default": "./dist/experimental/makeExperimental.js"
