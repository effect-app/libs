{
  "compilerOptions": {
    "plugins": [
      {
        "name": "ts-plugin-sort-import-suggestions",
        "moveUpPatterns": [
          "\\.{1,2}/",
          "^(?:\\.\\./)+",
          "^#",
          "^@/",
          "effect",
          "^@effect/"
        ],
        "moveDownPatterns": [
          "^node_modules/"
        ],
        "overrides": {
          "effect-app": []
        }
      },
      {
        "name": "@effect/language-service",
        "diagnosticSeverity": {
          "missingEffectServiceDependency": "error",
          "effectFnOpportunity": "warning",
        }
      }
    ]
  }
}