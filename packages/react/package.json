{
    "name": "@effect-ts-app/react",
    "version": "0.29.4",
    "license": "MIT",
    "dependencies": {
        "abort-controller": "^3.0.0",
        "cross-fetch": "^3.1.4",
        "path-parser": "^6.1.0"
    },
    "devDependencies": {
        "@effect-ts/core": "^0.42.5",
        "@effect-ts/monocle": "^0.34.1",
        "@effect-ts/schema": "^0.9.1",
        "@types/react": "^17.0.13",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "react": "^17.0.2"
    },
    "peerDependencies": {
        "@effect-ts/core": "^0.42.5",
        "@effect-ts/monocle": "^0.34.1",
        "@effect-ts/schema": "^0.9.1",
        "react": "^17.0.2"
    },
    "scripts": {
        "build": "yarn clean && tsc --build && tsc --build tsconfig.cjs.json && sh ../../utils/fix.sh && yarn generate-exports",
        "clean": "rm -rf ./_esm && find . | egrep '\\.m?js(\\.map)?$|\\.d.ts$|\\.tsbuildinfo' | grep -v .eslint | grep -v jest.config | xargs rm -f",
        "postpublish": "yarn clean",
        "generate-exports": "sh ../../utils/extract.sh > package.log",
        "prepare": "yarn build",
        "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./",
        "compile": "tsc --noEmit",
        "lint": "eslint .",
        "prettier": "prettier \"**/*.ts\"",
        "autofix": "yarn lint --fix && yarn prettier --write",
        "test": "echo 'no tests yet'",
        "testsuite": "yarn compile && yarn lint && yarn test && yarn circular"
    },
    "publishConfig": {
        "access": "public"
    },
    "module": "./_esm/index.js",
    "main": "./index.js",
    "typings": "./index.d.ts",
    "exports": {
        ".": "./index.mjs",
        "./data": "./data.mjs",
        "./hooks": "./hooks.mjs",
        "./context": "./context.mjs"
    },
    "sideEffects": false
}