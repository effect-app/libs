{
  "singleAttributePerLine": true,
  "htmlWhitespaceSensitivity": "strict",
  "vueIndentScriptAndStyle": true,
  "printWidth": 80,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSameLine": false,
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "parser": "vue",
        "singleAttributePerLine": true
      }
    }
  ]
} 
