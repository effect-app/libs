  "version": "2.0.0",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@effect/experimental": "^0.30.3",
    "@effect/sql": "^0.18.3",
    "effect-app": "workspace:*",
    "query-string": "^9.1.1"
  },
  "devDependencies": {
    "@azure/cosmos": "^4.1.1",
    "@azure/service-bus": "^7.9.5",
    "@types/express": "^5.0.0",
    "@types/redis": "^2.8.32",
    "@types/redlock": "^4.0.7",
    "mongodb": "6.10.0",
    "redis": "^3.1.2",
