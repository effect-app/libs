{
  "name": "@effect-ts-app/fluent",
  "version": "0.0.1",
  "license": "MIT",
  "exports": {
    "./_ext/*": {
      "module": "./dist/_ext/*.ts",
      "default": "./dist/cjs/_ext/*.js"
    },
    "./polyfill": {
      "module": "./dist/polyfill/*.ts",
      "default": "./dist/cjs/polyfill/*.js"
    },
    "./polyfill/*": {
      "module": "./dist/polyfill/*.ts",
      "default": "./dist/cjs/polyfill/*.js"
    },
    "./types/*": "./types/*.ts"
  },
  "devDependencies": {
    "@effect-ts/node": "^0.28.0"
  },
  "scripts": {
    "build": "tsc --build && tsc --build tsconfig.cjs.json",
    "watch": "tsc --build --watch",
    "watch2": "tsc -w",
    "prepare": "yarn build",
    "clean": "rm -rf dist",
    "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./src",
    "compile": "tsc --noEmit",
    "lint": "eslint ./src ./types",
    "prettier": "prettier \"src/**/*.ts\" \"types/**/*.d.ts\"",
    "autofix": "yarn lint --fix && yarn prettier --write",
    "test": "echo 'no tests yet'",
    "testsuite": "yarn lint && yarn test && yarn circular",
    "ncu": "ncu"
  }
}