{
  "name": "@effect-ts-app/pnpm-singleton",
  "version": "1.4.7",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm clean-dist && node ../../scripts/package.js && NODE_OPTIONS=--max-old-space-size=6144 tsc --build && pnpm build2:cjs",
    "build2:cjs": "rm -rf _cjs && babel dist --config-file ../../.babel.cjs.json --out-dir _cjs --out-file-extension .cjs --source-maps",
    "watch": "pnpm build --watch",
    "watch2": "pnpm clean-dist && NODE_OPTIONS=--max-old-space-size=6144 tsc -w",
    "clean": "rm -rf dist && rm -rf _cjs",
    "clean-dist": "sh ../../scripts/clean-dist.sh",
    "circular": "pnpm circular:src && pnpm circular:dist",
    "circular:src": "madge --circular --ts-config ./tsconfig.json --extensions ts ./_src",
    "circular:dist": "madge --circular --extensions js ./dist",
    "compile": "NODE_OPTIONS=--max-old-space-size=6144 tsc --noEmit",
    "lint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint ./_src",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx .",
    "autofix": "pnpm lint --fix",
    "test": "vitest",
    "test:run": "pnpm run test run --passWithNoTests",
    "testsuite": "pnpm lint && pnpm circular && pnpm run test:run",
    "ncu": "ncu -x @types/faker,faker,date-fns,react-hook-form",
    "pub": "pnpm prepublish && npm publish --access public",
    "prepublish": "pnpm build && cp -f ./tsconfig.json ./tsconfig.json.bak && node ../../scripts/mergeTsConfig.cjs ./tsconfig.json",
    "postpublish": "mv -f ./tsconfig.json.bak ./tsconfig.json && rm -f tsplus.config.json"
  },
  "resolutions": {
    "typescript": "https://github.com/0x706b/typescript/raw/79d32bc01c2ce3a56f06f6fc3ae27fc7f7a1f48a/typescript-5.0.0-tsplus.20221227.tgz"
  },
  "dependencies": {
    "@effect-ts-app/boilerplate-prelude": "workspace:*",
    "@effect-ts-app/react": "workspace:*",
    "@effect-ts-app/core": "workspace:*",
    "@effect-ts-app/schema": "*",
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "@effect-ts-app/compiler": "workspace:*",
    "@types/node": "~18.11.17",
    "@types/react": "^18.0.26",
    "react": "^18.2.0",
    "react-hook-form": "7.40.0",
    "react-intl": "^6.2.5",
    "ts-node": "^10.9.1",
    "typescript": "https://github.com/0x706b/typescript/raw/79d32bc01c2ce3a56f06f6fc3ae27fc7f7a1f48a/typescript-5.0.0-tsplus.20221227.tgz",
    "vitest": "^0.26.2"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./_cjs/index.cjs"
      }
    },
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./_cjs/index.cjs"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      },
      "require": {
        "types": "./dist/plugin.d.ts",
        "default": "./_cjs/plugin.cjs"
      }
    }
  },
  "gitHead": "bd8e27eea3eff97db8739d577d67e7336c078d28",
  "sideEffects": [
    "./dist/faker.js",
    "./faker",
    "./src/faker",
    "./dist/faker",
    "./dist/src/faker.js",
    "./dist/faker.js"
  ]
}