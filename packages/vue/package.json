  },
  "peerDependencies": {
    "@effect/platform": "^0.46.3",
    "@effect/platform-browser": "^0.30.10",
    "@effect/schema": "^0.63.2",
    "effect": "^2.4.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@effect-app/compiler": "workspace:*",
    "@rollup/pluginutils": "^5.1.0",
    "@types/node": "~20.11.24",
    "intl-messageformat": "^10.5.11",
    "json5": "^2.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
