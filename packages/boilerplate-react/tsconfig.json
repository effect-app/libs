{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "lib": [
            "esnext",
            "DOM"
        ],
        "tsPlusConfig": "../../tsplus.config.json",
        "tsPlusTypes": [
            "./node_modules/@effect-ts-app/core/vendor/effect-io-tsplus.json",
            "./node_modules/@effect-ts-app/core/vendor/effect-stm-tsplus.json",
            "./node_modules/@effect-ts-app/core/vendor/fp-ts-data-tsplus.json"
        ],
        "moduleResolution": "Node16",
        "outDir": "./dist",
        "rootDir": "./_src",
        "tsBuildInfoFile": "./dist/.tsbuildinfo",
    },
    "include": [
        "./_src/**/*.ts"
    ],
    "ts-node": {
        "require": [
            "tsconfig-paths/register"
        ],
        "compilerOptions": {
            "module": "CommonJS"
        }
    },
    "references": [
        {
            "path": "../boilerplate-prelude"
        },
        {
            "path": "../schema"
        },
        {
            "path": "../core"
        }
    ]
}